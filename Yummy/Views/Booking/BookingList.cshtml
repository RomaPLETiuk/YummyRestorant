@{
}



@model List<Yummy.Models.BookTable>
@*@model BookingVM;*@
@*@model List<Yummy.ViewModels.BookingVM>*@
<h2>Booking List</h2>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Phone</th>
            <th>Date</th>
            <th>Time</th>
            <th>Number of People</th>
            <th>Message</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var booking in Model)
        {
            <tr>
                <td>@booking.Name</td>
                <td>@booking.Email</td>
                <td>@booking.Phone</td>
                <td>@booking.Date.ToShortDateString()</td>
                <td>@booking.Time.ToShortTimeString()</td>
                <td>@booking.NumberOfPeople</td>
                <td>@booking.Message</td>
            </tr>
        }
    </tbody>
</table>

